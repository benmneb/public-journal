<%- include('partials/header') %>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for='post-title'>Title</label>
    <input class='form-control' id='post-title' type="text" name="postTitle">
    <label for='post-body'>Post</label>
    <textarea class='form-control' id='post-body' rows='7' type="text" name="postBody"></textarea>
  </div>
  <button class='btn btn-primary' type="submit" name="publish">Publish</button>
</form>

<%- include('partials/footer') %>
